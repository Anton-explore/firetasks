    "@types/lodash": "~4.14.184",
    "@types/node": "~18.7.11",
    "firebase-tools": "~11.8.0",
    "fuzzy": "^0.1.3",
    "inquirer": "~9.1.0",
    "inquirer-autocomplete-prompt": "~2.0.0",
    "jasmine-core": "~4.3.0",
    "jest": "~28.1.3",
    "jest-preset-angular": "~12.2.0",
    "jsonc-parser": "~3.1.0",
    "open": "~8.4.0",
    "typescript": "~4.7.4"
  }
