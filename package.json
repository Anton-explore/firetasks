    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.8.0",
    "jest": "^27.2.4",
    "jest-preset-angular": "^10.0.1",
    "jsonc-parser": "^3.0.0",
    "open": "^7.0.3",
    "typescript": "~4.6.4"
