{
  "name": "fire-tasks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:ng": "ng test",
    "install:all": "yarn install && yarn --cwd functions install",
    "start:emulators": "firebase --project=firetasks-local emulators:start --only auth,firestore",
    "apply:fixtures": "yarn --cwd functions cmd src/commands/runFixtures.ts",
    "deploy": "firebase deploy --only hosting,firestore"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.0.4",
    "@angular/cdk": "~15.0.4",
    "@angular/common": "~15.0.4",
    "@angular/compiler": "~15.0.4",
    "@angular/core": "~15.0.4",
    "@angular/fire": "~7.5.0",
    "@angular/flex-layout": "~14.0.0-beta.41",
    "@angular/forms": "~15.0.4",
    "@angular/material": "~15.0.4",
    "@angular/platform-browser": "~15.0.4",
    "@angular/platform-browser-dynamic": "~15.0.4",
    "@angular/router": "~15.0.4",
    "@ngneat/edit-in-place": "~1.6.1",
    "firebase": "~9.15.0",
    "lodash": "^4.17.21",
    "rxfire": "~6.0.3",
    "rxjs": "~7.8.0",
    "tslib": "~2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
