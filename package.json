    "@types/lodash": "~4.14.191",
    "@types/node": "~18.11.18",
    "firebase-tools": "~11.20.0",
    "fuzzy": "^0.1.3",
    "inquirer": "~9.1.4",
    "inquirer-autocomplete-prompt": "~3.0.0",
    "jasmine-core": "~4.5.0",
    "jest": "~29.3.1",
    "jest-preset-angular": "~12.2.3",
    "jsonc-parser": "~3.2.0",
    "open": "~8.4.0",
    "typescript": "4.8.4"
  }
